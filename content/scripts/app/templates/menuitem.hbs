<li class="span3 smooth-margin desktop-link" title="{{description}}" >
    <div class="thumbnail">
      {{#if cssIcon}}
        <i class="{{cssIcon}}"></i>
      {{/if}}
      {{#if osIcon}}
      <img src="/osicon/{{osIcon}}.png">
      {{/if}}
     <p class="item-name" >{{name}}</p>
    </div>
</li>



