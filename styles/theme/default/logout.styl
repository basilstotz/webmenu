
// Default theme

.logout-btn-container
  .action .item
    border-radius 5px
    border 1px solid #dcdcdc
    &:hover
      box-shadow inset 5px 5px 35px rgba(70,70,70,0.35), inset -5px -5px 35px rgba(70,70,70,0.35)
      cursor pointer
      background-color silver

  .js-other select, .action .item
    color black
    background-color silver
    font-size 12pt
    border 0px
    color black
    background-color silver

  select
    background-color transparent



